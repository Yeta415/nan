{
  "version": 3,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [

    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
 
    {
      "src": "/admin",
      "dest": "/admin.html"
    },

    {
      "src": "/",
      "dest": "/index.html"
    },

    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
